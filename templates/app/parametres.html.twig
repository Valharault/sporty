{% extends 'base.html.twig' %}

{% block title %}{{ app.user.username }}{% endblock %}

{% block body %}
    <main id="main-container">
        <div class="content">
            <div class="block block-themed">
                <div class="block-content">
                    <div class="col-md-12">
                        <div class="js-wizard-simple block">

                            <ul class="nav nav-tabs nav-tabs-alt nav-fill" role="tablist">
                                <li class="nav-item">
                                    <a class="nav-link active" href="#wizard-progress2-step1" data-toggle="tab">1. Détails personnels</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="#wizard-progress2-step2" data-toggle="tab">2. Détails sportifs</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="#wizard-progress2-step3" data-toggle="tab">3. Disponibilités</a>
                                </li>
                            </ul>

                            <form method="post">

                                <div class="block-content block-content-full tab-content" style="min-height: 274px;">

                                    <div class="tab-pane active" id="wizard-progress2-step1" role="tabpanel">
                                        {{ form_start(form_details) }}
                                        <div class="form-group">
                                            <div class="form-material floating">
                                                {{ form_widget(form_details.ville, {'value' : app.user.details.ville }) }}
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="form-material floating">
                                                {{ form_widget(form_details.codepostal, {'value' : app.user.details.codepostal}) }}
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="form-material floating">
                                                {{ form_widget(form_details.telephone, {'value' : app.user.details.telephone}) }}
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="form-material floating">
                                                {{ form_widget(form_details.bio, {'value' : app.user.details.bio}) }}
                                            </div>
                                        </div>
                                        <div class="block-content block-content-sm block-content-full bg-body-light">
                                            <div class="row">
                                                <div class="col-6">
                                                </div>
                                                <div class="col-6 text-right">
                                                    <button type="submit" class="btn btn-alt-success" data-wizard="finish">
                                                        Mettre à jour <i class="fa fa-check-circle ml-5"></i>
                                                    </button>
                                                </div>
                                            </div>
                                        </div>
                                        {{ form_end(form_details) }}
                                    </div>

                                    <div class="tab-pane" id="wizard-progress2-step2" role="tabpanel">
                                        {{ form_start(form_sport) }}
                                        <div class="block">
                                            <div class="row">
                                                {% for sport in form_sport.children.nom.children%}
                                                    <div class="col-md-2 mt-3 mb-3">
                                                        <div class="custom-control custom-block custom-control-success" data-children-count="1">
                                                            <input type="checkbox" class="custom-control-input" id="{{ sport.vars.id }}" name="{{ sport.vars.full_name }}" value="{{ sport.vars.name }}">
                                                            <label class="custom-control-label" for="{{ sport.vars.id }}">
                                                            <span class="d-block text-center">
                                                                {{ sport.vars.label }}
                                                            </span>
                                                            </label>
                                                            <span class="custom-block-indicator">
                                                                <i class="fa fa-check"></i>
                                                            </span>
                                                        </div>
                                                    </div>
                                                {% endfor %}
                                            </div>
                                        </div>
                                        <div class="block-content block-content-sm block-content-full bg-body-light">
                                            <div class="row">
                                                <div class="col-6">
                                                </div>
